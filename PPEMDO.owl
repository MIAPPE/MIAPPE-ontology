<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/ppeo/PPEMDO.owl#"
     xml:base="http://purl.org/ppeo/PPEMDO.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:MDO="http://purl.org/ppeo/MDO.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:PPEO="http://purl.org/ppeo/PPEO.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://purl.org/ppeo/PPEMDO.owl">
        <owl:imports rdf:resource="http://purl.org/ppeo/MDO.owl"/>
        <owl:imports rdf:resource="http://purl.org/ppeo/PPEO.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ppeo/MDO.owl#mayContainAtLeastOne -->

    <owl:AnnotationProperty rdf:about="http://purl.org/ppeo/MDO.owl#mayContainAtLeastOne"/>
    


    <!-- http://purl.org/ppeo/MDO.owl#mayContainExactlyOne -->

    <owl:AnnotationProperty rdf:about="http://purl.org/ppeo/MDO.owl#mayContainExactlyOne"/>
    


    <!-- http://purl.org/ppeo/MDO.owl#takes_values_from -->

    <owl:AnnotationProperty rdf:about="http://purl.org/ppeo/MDO.owl#takes_values_from"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ppeo/PPEMDO.owl#Observation_unit_factor_value -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEMDO.owl#Observation_unit_factor_value">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#restriction_field"/>
        <MDO:references rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalFactorValue"/>
        <MDO:hasDefinition xml:lang="en">List of values for each factor applied to the observation unit.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Observation Unit factor value</MDO:hasDisplayName>
        <MDO:isTail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isTail>
        <MDO:hasExample xml:lang="en">Watered</MDO:hasExample>
        <rdfs:label xml:lang="en">Observation unit factor value</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#biological_material -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#biological_material">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#section"/>
        <MDO:contains rdf:resource="http://purl.org/ppeo/PPEO.owl#hasPreprocessing"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialAltitude"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialCoordinatesUncertainty"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLatitude"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLongitude"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasGenus"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasInfraspecificName"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasSpecies"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#material_source"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialID"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasOrganism"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#environment"/>
        <MDO:hasDefinition xml:lang="en">The biological material being studied (e.g. plants grown from a certain bag or seed, or plants grown in a particular field). The original source of that material (e.g., the seeds or the original plant cloned) is called the material source, which, when held by a material repository, should have its stock identified.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological Material</MDO:hasDisplayName>
        <MDO:mayContainAtLeastOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasPreprocessing"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#environment"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialAltitude"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialCoordinatesUncertainty"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLatitude"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLongitude"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasGenus"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasInfraspecificName"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasSpecies"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#data_file -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#data_file">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#section"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasDataFileDescription"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasDataFileLink"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasDataFileVersion"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#biological_material"/>
        <MDO:hasDefinition xml:lang="en">A file or digital object holding observation data recorded during one or more assays of the study, typically in tabular form. Multiple data files may be provided per study, and each file can include observations for several observation units and several observed variables.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Data file</MDO:hasDisplayName>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#environment -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#environment">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#section"/>
        <MDO:contains rdf:resource="http://purl.org/ppeo/PPEO.owl#environment_parameter"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#factor"/>
        <MDO:hasDefinition xml:lang="en">Environmental parameters that were kept constant throughout the study and did not change between observation units or assays. Environment characteristics that vary over time, i.e. environmental variables, should be recorded as Observed Variables (see below).</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Environment</MDO:hasDisplayName>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#environment_parameter -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#environment_parameter">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#object"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEnvironmentParameterName"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEnvironmentParameterValue"/>
        <MDO:hasDisplayName xml:lang="en">Environment parameter</MDO:hasDisplayName>
        <MDO:isHead rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isHead>
        <MDO:isTail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isTail>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#event -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#event">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#section"/>
        <MDO:containsAtLeastOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventDateTime"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventAccessionNumber"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventDescription"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventType"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#observed_variable"/>
        <MDO:includes rdf:resource="http://purl.org/ppeo/PPEO.owl#observation_unit"/>
        <MDO:hasDefinition xml:lang="en">An event is discrete occurrence at a particular time in the experiment (which can be natural, such as rain, or unnatural, such as planting, watering, etc). Events may be the realization of Factors or parts of Factors, or may be confounding to Factors. Can be applied at the whole study level or to only a subset of observation units.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Event</MDO:hasDisplayName>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventAccessionNumber"/>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasEventDescription"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#factor -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#factor">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#section"/>
        <MDO:containsAtMostOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalFactorDescription"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#factor_value"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalFactorType"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#observation_unit"/>
        <MDO:hasDefinition xml:lang="en">The object of a study is to ascertain the impact of one or more factors on the biological material. Thus, a factor is, by definition a condition that varies between observation units, which may be biotic (pest, disease interaction) or abiotic (treatment and cultural practice) in nature. Depending on the level of the data, an experimental factor can be either  &quot;what is the factor applied to the plant&quot; (i.e. Unwatered), or the &quot;environmental characterisation&quot; (i.e. if no rain on unwatered plant : Drought ;  if rain on unwatered plant: Irrigated)</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Experimental Factor</MDO:hasDisplayName>
        <MDO:mayContainExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalFactorDescription"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#factor_value -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#factor_value">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#object"/>
        <MDO:containsExactlyOne rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalFactorValue"/>
        <MDO:hasDisplayName xml:lang="en">Factor values</MDO:hasDisplayName>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasAssociatedPublication -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasAssociatedPublication">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#long_text_field"/>
        <MDO:hasDefinition xml:lang="en">An identifier for a literature publication where the investigation is described. Use of DOIs is recommended.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Associated publication(s)</MDO:hasDisplayName>
        <MDO:isTail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isTail>
        <MDO:hasExample>doi:10.1371/journal.pone.0071377</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialAltitude -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialAltitude">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialCoordinatesUncertainty"/>
        <MDO:hasDefinition xml:lang="en">Altitude of the studied biological material, provided in meters (m). [Alternative identifier for in situ material]</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological Materal altitude</MDO:hasDisplayName>
        <MDO:hasExample>10 m</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialCoordinatesUncertainty -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialCoordinatesUncertainty">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasPreprocessing"/>
        <MDO:hasDefinition xml:lang="en">Circular uncertainty of the coordinates, preferably provided in meters (m). [Alternative identifier for in situ material]</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological material coordinates preprocessing</MDO:hasDisplayName>
        <MDO:hasExample>200 m</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialID -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialID">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#ID_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasOrganism"/>
        <MDO:hasDefinition xml:lang="en">Code used to identify the biological material in the data file. Should be unique within the Investigation. Can correspond to experimental plant ID, seed lot ID, etc… This material identification is different from a BiosampleID which corresponds to Observation Unit or Samples sections below.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological material ID</MDO:hasDisplayName>
        <MDO:isHead rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isHead>
        <MDO:hasExample>INRA:W95115_inra_2001; INRA:inra_kernel_2351; Rothamsted:rres_GK090847</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLatitude -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLatitude">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#fixed_datatype_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLongitude"/>
        <MDO:hasDefinition xml:lang="en">Latitude of the studied biological material. [Alternative identifier for in situ material]</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological material latitude</MDO:hasDisplayName>
        <MDO:hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">+39.067</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLongitude -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialLongitude">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#fixed_datatype_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasBiologicalMaterialAltitude"/>
        <MDO:hasDefinition xml:lang="en">Longitude of the studied biological material. [Alternative identifier for in situ material]</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Biological material longitude</MDO:hasDisplayName>
        <MDO:hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">-8.73</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasCollectionDate -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasCollectionDate">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#fixed_datatype_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasSampleExternalID"/>
        <MDO:hasDefinition xml:lang="en">The date and time when the sample was collected / harvested</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Collection date</MDO:hasDisplayName>
        <MDO:hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2005-08-15T15:52:01+00:00</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasContactInstitution -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasContactInstitution">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#free_value_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasCountryName"/>
        <MDO:hasDefinition xml:lang="en">Name and address of the institution responsible for the study.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Contact institution</MDO:hasDisplayName>
        <MDO:hasExample>UMR de Génétique Végétale, INRA – Université Paris-Sud – CNRS, Gif-sur-Yvette, France</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasCountryName -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasCountryName">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#free_value_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasExperimentalSiteName"/>
        <MDO:hasDefinition xml:lang="en">The country where the experiment took place, either as a full name or preferably as a 2-letter code.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Geographic location (country)</MDO:hasDisplayName>
        <MDO:hasExample>FR</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasCulturalPractices -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasCulturalPractices">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#long_text_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasMapOfExperimentalDesign"/>
        <MDO:hasDefinition xml:lang="en">General description of the cultural practices of the study.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Cultural practices</MDO:hasDisplayName>
        <MDO:hasExample xml:lang="en">Irrigation was applied according needs during summer to prevent water stress.</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasDataFileDescription -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasDataFileDescription">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#long_text_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasDataFileVersion"/>
        <MDO:hasDefinition xml:lang="en">Description of the format of the data file. May be a standard file format name, or a description of organization of the data in a tabular file.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Data file description</MDO:hasDisplayName>
        <MDO:isTail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isTail>
        <MDO:hasExample>FASTA
tab-delimited
column headers headers: 1. A 2. B 3. C</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasDataFileLink -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasDataFileLink">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasDataFileDescription"/>
        <MDO:hasDefinition xml:lang="en">Link to the data file (or digital object) in a public database or in a persistent institutional repository; or identifier of the data file when submitted together with the MIAPPE submission.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Data file link</MDO:hasDisplayName>
        <MDO:isHead rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isHead>
        <MDO:hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.ebi.ac.uk/arrayexpress/experiments/E-GEOD-32551/</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasDataFileVersion -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasDataFileVersion">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#field"/>
        <MDO:hasDefinition xml:lang="en">The version of the dataset (the actual data).</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Data file version</MDO:hasDisplayName>
        <MDO:isTail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</MDO:isTail>
        <MDO:hasExample rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasDescriptionOfGrowthFacility -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasDescriptionOfGrowthFacility">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#free_value_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasTypeOfGrowthFacility"/>
        <MDO:hasDefinition xml:lang="en">Short description of the facility in which the study was carried out.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Description of growth facility</MDO:hasDisplayName>
        <MDO:hasExample xml:lang="en">field environment condition
NA</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasDescriptionOfTheExperimentalDesign -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasDescriptionOfTheExperimentalDesign">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#long_text_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasTypeOfExperimentalDesign"/>
        <MDO:hasDefinition xml:lang="en">Short description of the experimental design, possibly including statistical design. In specific cases, e.g. legacy datasets or data computed from several studies, the experimental design can be &quot;unknown&quot;/&quot;NA&quot;, &quot;aggregated/reduced data&quot;, or simply &apos;none&apos;.</MDO:hasDefinition>
        <MDO:hasDisplayName xml:lang="en">Description of the experimental design</MDO:hasDisplayName>
        <MDO:hasExample xml:lang="en">Lines were repeated twice at each location using a complete block design. In order to limit competition effects, each block was organized into four sub-blocks corresponding to earliness groups based on a priori information.</MDO:hasExample>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/ppeo/PPEO.owl#hasEmailContact -->

    <owl:NamedIndividual rdf:about="http://purl.org/ppeo/PPEO.owl#hasEmailContact">
        <rdf:type rdf:resource="http://purl.org/ppeo/MDO.owl#free_value_field"/>
        <MDO:hasNext rdf:resource="http://purl.org/ppeo/PPEO.owl#hasPersonID"/>
        <MDO:hasDefinition xml:lang="en">The electronic mail address of the person.</MDO:hasDefinition>
